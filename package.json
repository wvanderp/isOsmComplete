{
  "name": "is-osm-complete",
  "version": "0.1.0",
  "description": "A website for checking completeness of a osm",
  "author": "wvdp",
  "license": "MIT",
  "homepage": "https://wvanderp.github.io/isOsmComplete/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wvanderp/isOsmComplete"
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "staticFiles": {
    "staticPath": "data"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@types/chai": "^4.3.17",
    "@types/crypto-js": "^4.2.2",
    "@types/d3": "^7.4.3",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/showdown": "^2.0.6",
    "@types/unzipper": "^0.10.10",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "chai": "^5.1.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "parcel": "2.12.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "expect": "^29.7.0"
  },
  "dependencies": {
    "@rapideditor/country-coder": "^5.3.0",
    "ajv": "^8.17.1",
    "axios": "^1.7.4",
    "bootstrap": "^5.3.3",
    "country-code-emoji": "^2.3.0",
    "country-flag-emoji-polyfill": "^0.1.8",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.5.6",
    "d3": "^7.9.0",
    "extract-zip": "^2.0.1",
    "i18n-iso-countries": "^7.11.3",
    "overpass-ts": "^4.3.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactstrap": "^9.2.2",
    "showdown": "^2.1.0",
    "url-join": "^5.0.0"
  },
  "scripts": {
    "app": "npm run clean && parcel serve app/index.html -p 3001",
    "build": "npm run clean &&  parcel build --no-source-maps --public-url /isOsmComplete --dist-dir ./gitBuild app/index.html && ts-node ./scripts/postProcess.ts",
    "lint": "npm-run-all -c -s tsc eslint",
    "lintData": "ts-node -T collect/lint/lint.ts",
    "eslint": "eslint --ext ts,tsx,js app/ collect/",
    "tsc": "tsc --noEmit",
    "collect": "ts-node collect && npm run lintData",
    "clean": "rimraf .parcel-cache dist gitBuild",
    "test": "mocha --recursive tests/**/*.spec.ts tests/**/**/*.spec.ts"
  }
}
